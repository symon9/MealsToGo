{
  "expo": {
    "name": "MealsToGo",
    "slug": "MealsToGo",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "description": "MealsToGo is a modern food discovery app that lets users explore restaurants by city, view locations, and manage their favourites with Firebase auth.",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.io"
    },
    "plugins": ["expo-font"]
  }
}
